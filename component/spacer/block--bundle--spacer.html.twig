{# ## Spacer ## #}

{# Attach library #}
{{ attach_library('cua_theme/spacer') }}

{# Render block content #}
<div{{ attributes }}>
    <div class="spacer {{ size }}">
        {{ title_prefix }}
        <div class="small text-align-center"> {{ 'Blank space size:'|t ~ ' ' ~ size ~ ' ' ~ '(Only visible for logged in ADMIN)'|t }}</div>
        {{ title_suffix }}
    </div>
</div>
