{# extends @bootstrap_barrio/layout/block.html.twig
/**
 * @file
 * Theme override for tabs.
 */
#}
{% block content %}
  {% if content %}
      <div class="container text-center">
          <h4>- Edit Page Content Below -</h4>
          <p>Layout Builder Tutorial: <a href="https://www.drupal.org/docs/extending-drupal/contributed-modules/contributed-module-documentation/layout-builder/how-to-layout-builder" target="_blank">More Information</a></p>
      </div>

      <nav class="tabs" role="navigation" aria-label="{{ 'Tabs'|t }}">
      {{ content }}
    </nav>
  {% endif %}
{% endblock %}
